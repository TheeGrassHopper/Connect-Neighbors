<%= simple_form_for @paperclip_image,  html: { multipart: true } do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
  </div>
  <div class = "form-group">
  	<%= f.label :image %>
  <div class="btn btn-default">
   <%= f.file_field :paperclip_image, html: { class: 'form-control' }%>
  </div>
  	<% if f.object.image? %>
  	<br/>
  	<%= image_tag(f.object.image.url(:small)) %> <br/>
  	<em>Currently Uploaded Image</em>
  	<% end %> 

  <div class="actions">
    <%= f.submit 'Save', class: "btn btn-primary" %>
  </div>
<% end %>
