<%= simple_form_for(@profile) do |f| %>
  <%= f.error_notification %>

  <p>
    <strong>User:</strong>
    <%= @profile.user %>
  </p>

  <div class="form-inputs">
    
    <%= f.label :paperclip_image %>
     <%= f.file_field :paperclip_image, html: { class: 'form-control' }%>
    <%= f.input :firstname %>
    <%= f.input :lastname %>
    <%= f.input :apt %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>



